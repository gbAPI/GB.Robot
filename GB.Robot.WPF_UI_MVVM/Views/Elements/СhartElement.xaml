<UserControl x:Class="GB.Robot.WPF_UI_MVVM.Views.Elements.СhartElement"
             
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GB.Robot.WPF_UI_MVVM.Views.Elements"
             DataContext="{Binding AdministratorWindowModel,Source={StaticResource Locator}}"
             xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
             
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    
    <Grid>
        <lvc:CartesianChart 
            DisableAnimations="True" 
            Hoverable="False" 
            DataTooltip="{x:Null}">
            <lvc:CartesianChart.Series>
                <lvc:LineSeries x:Name="LineChart" Values="{Binding CValue}" Fill="#FF6B1493"/>
            </lvc:CartesianChart.Series>
            <lvc:CartesianChart.AxisY>
                <lvc:Axis Title="Количество запросов" MinValue="-5"/>
            </lvc:CartesianChart.AxisY>
            <lvc:CartesianChart.AxisX>
                <lvc:Axis Labels=""/>
            </lvc:CartesianChart.AxisX>
        </lvc:CartesianChart>
    </Grid>
</UserControl>
